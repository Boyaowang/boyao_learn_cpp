{
    topWall
    {
        type            zeroGradient;
    }
    bottomWall
    {
        type            codedFixedValue;
        value           uniform 293;
        name            heating;
        code            #{
            const scalar t = this->db().time().value();
            const scalar Ttemp = 273 + 10*t;
            operator==(Ttemp);
        #};
    }
    sideWallL
    {
        type            symmetry;
    }
    sideWallR
    {
        type            fixedValue;
