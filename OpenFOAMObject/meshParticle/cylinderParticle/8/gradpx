FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "8";
    object      gradpx;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    topWall
    {
        type            calculated;
        value           uniform 0;
    }
    bottomWall
    {
        type            calculated;
        value           uniform 0;
    }
    sideWallL
    {
        type            symmetry;
    }
    sideWallR
    {
        type            calculated;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
}


