FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(T|p)"
    {
/*        solver          smoothSolver;*/
/*        smoother        symGaussSeidel;*/
/*        tolerance       1e-05;*/
/*        relTol          0.1;*/
      solver          GAMG;
      smoother        DICGaussSeidel;
      tolerance       1e-08;
      relTol          0.1;
    }

    "(rhos|rhois|rhoc|rhot1|rhot2|rhog|rhoAr)"
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-06;
        relTol          0;
    }

}


SIMPLE
{
    nNonOrthogonalCorrectors 2;
}


