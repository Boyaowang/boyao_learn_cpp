FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1.0e5;

boundaryField
{
    //front
    wallProlongation
    {
        type            zeroGradient;
    }
    //back
    wallDiffuser
    {
        type            zeroGradient;
    }
    //wall
    statSwirlWall
    {
        type            zeroGradient;
    }
    //porosityWall
    rotSwirlWall
    {
        type            zeroGradient;
    }

    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            fixedValue;
        value           $internalField;
    }
}

// ************************************************************************* //
