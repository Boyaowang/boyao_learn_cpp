FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1;

boundaryField
{

    //front
    wallProlongation
    {
        type            zeroGradient;
    }
    //back
    wallDiffuser
    {
        type            zeroGradient;
    }
    //wall
    statSwirlWall
    {
        type            zeroGradient;
    }
    //porosityWall
    rotSwirlWall
    {
        type            zeroGradient;
    }

    inlet
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.05;
        value           $internalField;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

}
// ************************************************************************* //
